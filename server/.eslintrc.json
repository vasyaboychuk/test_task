{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error",4],
        "max-len": ["error", 130],
        "no-unused-vars": "warn",
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "prettier/prettier": ["error", { "singleQuote": true, "trailingComma": "es5" }]
    }
}
